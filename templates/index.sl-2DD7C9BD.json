    "9c541f33-454a-4c57-a527-46101f7db223": {
      "type": "custom-liquid",
      "disabled": true,
      "settings": {
        "custom_liquid": "<style>\n    .custom-banner {\n        display: flex;\n        grid-template-columns: 1fr 1fr;\n        overflow: hidden;\n        width: 100vw;\n        position: relative;\n    }\n\n    .banner-title {\n        position: absolute;\n        top: 0;\n        left: 0;\n        color: white;\n        z-index: 1;\n        width: 100%;\n        text-align: center;\n        font-size: 40px;\n    }\n\n    .custom-banner .column {\n        align-items: center;\n        width: 100%;\n        display: flex;\n        justify-content: center;\n        align-items: flex-end;\n        background-repeat: no-repeat !important;\n        position: relative;\n        transition: .3s;\n        text-decoration: none;\n        padding: 3rem;\n        overflow: hidden;\n        cursor: pointer;\n    }\n\n    .custom-banner span {\n        background: #DB0032;\n        border-radius: 50px;\n        color: white;\n        padding: 1rem 3rem;\n        font-size: 24px;\n        font-weight: 600;\n        transition: 0.3s;\n        text-align: center;\n        text-transform: uppercase;\n        box-shadow: 0 0 7px 5px #0000004f;\n\n    }\n\n    .custom-banner span:hover {\n        background: #DB0032;\n    }\n\n    .custom-banner .for-her {\n        background: url('https:\/\/cdn.shopify.com\/s\/files\/1\/0806\/4391\/6087\/files\/opti_web_new_1.png?v=1703657881');\n        background-size: cover;\n        background-position: center center;\n    }\n\n    .custom-banner .for-him {\n        background: url('https:\/\/cdn.shopify.com\/s\/files\/1\/0806\/4391\/6087\/files\/opti_web_new_2.png?v=1703657881');\n        background-size: cover;\n        background-position: center center;\n        position: relative;\n    }\n\n    @media screen and (min-width: 992px) {\n        .custom-banner {\n            height: 75vh;\n        }\n\n        .bundle2 {\n            display: none;\n        }\n    }\n\n    .bundle-button {\n        position: absolute;\n        top: 50%;\n        left: 50%;\n        transform: translate(-50%, -50%);\n        width: 220px;\n        text-align: center;\n        border-radius: 100%;\n        z-index: 1;\n    }\n\n    .bundle2 {\n        right: unset;\n        top: 0;\n        left: 1rem important;\n        right: unset !important;\n    }\n\n    @media screen and (max-width: 992px) {\n        .custom-banner {\n            flex-direction: column;\n        }\n\n        .banner-title {\n            display: none;\n        }\n\n        .custom-banner .column {\n            height: 340px;\n            padding-bottom: 2rem;\n            background-size: cover;\n            padding: 2rem;\n        }\n\n        .custom-banner .column img {\n            width: 120px;\n        }\n\n        .custom-banner span {\n            margin: 0;\n        }\n\n        .bundle-button {\n            width: 120px;\n            top: 1rem;\n            right: 1rem;\n            left: unset;\n            transform: translate(0);\n        }\n\n        .bundle2 {\n            display: block !important;\n            left: 1rem;\n        }\n\n        .custom-banner span {\n            font-size: 18px;\n        }\n    }\n<\/style>\n\n<div class=\"custom-banner\">\n    <h1 class=\"banner-title\">LOSE WEIGHT FAST <br>& SAVE TODAY<\/h1>\n    <div class=\"column for-her\" onclick=\"location.href='\/collections\/shopall\/products\/buy-3-get-1-free-optislim-for-her-bundle-1'\">\n\n        <span>Shop For Her<\/span>\n\n    <\/div>\n\n    <div class=\"column for-him\" onclick=\"location.href='\/collections\/bundles\/products\/buy-3-get-1-free-optiman-for-him-bundle'\">\n\n        <span>Shop For Him<\/span>\n\n        <a href='collections\/bundles' class=\"bundle-button bundle2\"><img\n                src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0806\/4391\/6087\/files\/ROUNDLE_1.png?v=1703654937\" \/>\n        <\/a>\n    <\/div>\n\n    <a href=\"\/collections\/bundles\"><img class=\"bundle-button\"\n            src=\"https:\/\/cdn.shopify.com\/s\/